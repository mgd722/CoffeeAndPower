.notifications
  - if notice
    #notice_wrapper
      %p.notice= notice
  - elsif alert
    #alert_wrapper
      %p.alert= alert

.container.top-50
  .row 
    .col-md-8
      .panel
        .panel-heading Edit cafe
        .panel-body
          = render 'form'
      = link_to "Cancel", cafe_path,  class: "btn btn-default"